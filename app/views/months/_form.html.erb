<%= form_with model: month do |form| %>
    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
      <% month.errors.full_messages_for(:title).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
    <div>
      <%= form.label :status %><br>
      <%= form.select :status, ['public', 'private', 'archived'],
    selected: 'public' %>
    </div>
  <% end %>